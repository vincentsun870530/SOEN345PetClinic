    "version": "0.2.0",
    "configurations": [
        {
            "type": "java",
            "name": "CodeLens (Launch) - ConsistencyChecker",
            "request": "launch",
            "mainClass": "org.springframework.samples.petclinic.consistencychecker.ConsistencyChecker",
            "projectName": "spring-petclinic"
        },
        {
            "type": "java",
            "name": "Debug (Launch) - Current File",
            "request": "launch",
            "mainClass": "${file}"
        },
        {
            "type": "java",
            "name": "Debug (Launch)-PetClinicApplication<spring-petclinic>",
            "request": "launch",
            "mainClass": "org.springframework.samples.petclinic.PetClinicApplication",
            "projectName": "spring-petclinic"
        }
    ]
}